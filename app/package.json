{
  "name": "template",
  "version": "0.2.2",
  "scripts": {
    "start": "npm run server",
    "development": "nodemon -L . & NODE_ENV=development webpack --watch",
    "debug": "nodemon --inspect & NODE_ENV=development webpack --watch",
    "docker": "docker-compose rm -f && docker-compose up",
    "production": "npm run clean && NODE_ENV=production webpack -p",
    "upgrade": "npm version patch --sign-git-tag=true",
    "build": "webpack",
    "server": "forever .",
    "test": "npm clean",
    "mqtt": "bash ../mqtt/bin/run.sh",
    "mongo": "mkdir server/mongodb && npm run mongodb",
    "mongodb": "sudo mongod --dbpath server/mongodb --port 3001",
    "clean_modules": "modclean -r",
    "clean": "rimraf ./server/public/",
    "webpack": "npm run clean && webpack-dev-server --progress --inline --hot --port 3000"
  },
